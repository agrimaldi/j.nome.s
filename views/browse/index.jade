.page
  h1 Browsing database #{dataset}

  #appnavigation
    !=partial('appnavigation')

  #browse
    form(method='post', name='browse')
      !=partial('fields', { seqid: seqid, start: start, end: end })
      !=partial('trackselector', { tracks: config[dataset].tracks})
      div
        input(type='submit', value='Go', id='submit')

  #landmarks
    !=partial('landmarks', { dataset: dataset })

  #tracks
    #{data}
